@article{ ISI:000263491900007,
Author = {Lahaie, Ulysses David},
Title = {{Is Nursing Ready for WebQuests?}},
Journal = {{JOURNAL OF NURSING EDUCATION}},
Year = {{2008}},
Volume = {{47}},
Number = {{12}},
Pages = {{567-570}},
Month = {{DEC}},
Abstract = {{Based on an inquiry-oriented framework, WebQuests facilitate the
   construction of effective learning activities. Developed by Bernie Dodge
   and Tom March in 1995 at the San Diego State University, WebQuests have
   gained worldwide popularity among educators in the kindergarten through
   grade 12 educational sector. However, their application at the college
   and university levels is not well documented. WebQuests enhance and
   promote higher order-thinking skills, are consistent with Bloom's
   Taxonomy, and reflect a learner-centered instructional methodology
   (constructivism). They are based on solid theoretical foundations and
   promote critical thinking, inquiry, and problem solving. There is a role
   for WebQuests in nursing education. A WebQuest example is described in
   this article.}},
