@article{ ISI:000481992000005,
Author = {Anh Nguyet Diep and Zhu, Chang and Cocquyt, Celine and De Greef, Maurice
   and Minh Hien Vo and Vanwing, Tom},
Title = {{Adult learners' needs in online and blended learning}},
