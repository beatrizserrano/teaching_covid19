@BOOK{Grenfell2005384,
author={Grenfell, M.},
title={Knowledge construction online: Does constructivism deliver the goods?},
journal={Selected Styles in Web-Based Educational Research},
year={2005},
pages={384-402},
doi={10.4018/978-1-59140-732-4.ch024},
note={cited By 0},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-84898214946&doi=10.4018%2f978-1-59140-732-4.ch024&partnerID=40&md5=43b5432ab7d1af0969f4aca108b60777},
affiliation={School of Education, Charles Darwin University, Australia},
abstract={This chapter uses a contestatory style of research to challenge a number of our dearly held beliefs about constructivism, drawing on data from a discussion board which was an assessable component of a master's course, EME501: Redefining the Urban-Rural Divide. After reviewing the minimalist and constructivist approaches employed, it looks closely at the kinds of knowledge constructed and the challenges this poses for social constructivism. In particular it looks at the ways existing beliefs and attitudes are reinforced, the maintenance of conservative views, the persistence of gendered knowledge, and the emergence of irreconcilable differences with regard to ICT and the virtual classroom. While not invalidating constructivism as an overall approach to Web-mediated learning, the way in which it is implemented can give cause for concern. © 2006, Idea Group Inc.},
