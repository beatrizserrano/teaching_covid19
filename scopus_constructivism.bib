@ARTICLE{Eryilmaz2014303,
author={Eryilmaz, E. and Chiu, M.M. and Thoms, B. and Mary, J. and Kim, R.},
title={Design and evaluation of instructor-based and peer-oriented attention guidance functionalities in an open source anchored discussion system},
