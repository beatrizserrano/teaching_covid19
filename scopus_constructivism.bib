@CONFERENCE{Cuthell2005529,
author={Cuthell, J.},
title={Beyond collaborative learning: Communal construction of knowledge in an online environment},
journal={WEBIST 2005 - 1st International Conference on Web Information Systems and Technologies, Proceedings},
