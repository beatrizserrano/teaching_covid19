@CONFERENCE{Sun2006383,
author={Sun, L. and Ousmanou, K.},
title={Engineering user requirements for personalised information provision in life-long learning: A semiotic approach},
journal={Proceedings of the European Conference on Games-based Learning},
year={2006},
volume={2006-January},
pages={383-396},
note={cited By 0; Conference of 5th European Conference on e-Learning, ECEL 2006 ; Conference Date: 11 September 2006 Through 12 September 2006;  Conference Code:113223},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-84938589996&partnerID=40&md5=421ace5c009ae57dcbe7b30caa4b689b},
affiliation={Informatics Research Centre, The University of Reading, United Kingdom},
abstract={The knowledge economy and life-long learning have brought the need to support diverse communities of users with different learning needs, preferences and educational backgrounds. It is widely acknowledged that the ability of web-based learning services to deploy information content that effectively assist personalised knowledge construction requires the specification derived from users' information requirements. However, incorporating these requirements to support information content design strategies still remains a challenge. A method rooted in the constructivism and semiotic paradigms is presented in this paper. The method applies Organisational Semiotics techniques to analyse and represent the domain knowledge of personalised information provision in life long learning at the social level, hence assisting the modelling of users' requirements. The method features that users can express their needs, which in turn can be articulated into user's requirements. The user's requirements are captured in a User Requirements Space as they are transformed onto profile. An instance of the user requirements space is then mapped onto an Information Space, which will determine information content selection, their instructive sequencing and appropriate presentation forms. The method will further produce a set of information provision specifications for configuring information packages tailored to users' specific requirements. A case study is used to illustrate and validate the features of the method.},
author_keywords={E-learning;  Life long learning;  Personalisation;  Profiling;  User requirements elicitation},
